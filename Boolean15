import java.util.Scanner;


public class Boolean15 {

    public static void main(String[] args) {

        Scanner scanner = new Scanner(System.in);

        System.out.print("Sonni kiriting: ");
        int A = scanner.nextInt();

        System.out.print("Sonni kiriting: ");
        int B = scanner.nextInt();

        System.out.print("Sonni kiriting: ");
        int C = scanner.nextInt();

        if (A>0 && B>0) {
            System.out.println("A va B sonlar musbat ");
        } else if (A>0 && C>0) {
            System.out.println("A va C sonlar musbat ");
        }else if (B>0 && C>0) {
            System.out.println("B va C sonlar musbat ");
        } else {
            System.out.println("Sonlarni hech qaysi biri musbat emas");
        }
    }
}
