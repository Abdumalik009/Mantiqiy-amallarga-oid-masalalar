import java.util.Scanner;

public class Boolean23 {

    public static void main(String[] args) {

        Scanner scanner = new Scanner(System.in);

        System.out.print("Uch xonali  sonni kiriting: ");
        int A = scanner.nextInt();


        int yuz = A/100;
        int on =(A/10)%10;
        int bir =A%10;
        if (yuz == bir){
            System.out.println("Ushbu sonni chapdan o'qiganda ham, o'ngdan o'qiganda ham bir xil ");
        }else{
            System.out.println("Ushbu sonni chapdan o'qiganda ham, o'ngdan o'qiganda ham bir xil emas");
        }
    }
}
